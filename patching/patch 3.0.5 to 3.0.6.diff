+    return {
+      'enableLargeCategories': enableLargeCategories,
+    };
+  }
+
+  factory CategoryConfig.fromJson(Map<String, dynamic> map) {
+    return CategoryConfig(
+      enableLargeCategories: map['enableLargeCategories'] ?? false,
+    );
+  }
+}
